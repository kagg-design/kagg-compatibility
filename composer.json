{
  "name": "kagg/compatibility",
  "description": "KAGG Compatibility",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "compatibility",
    "PHP 8.1",
    "development",
    "deprecation",
    "notice"
  ],
  "homepage": "https://wordpress.org/plugins/kagg-compatibility/",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "KAGG Design",
      "email": "info@kagg.eu",
      "homepage": "https://kagg.eu/en/"
    }
  ],
  "support": {
    "issues": "https://wordpress.org/support/plugin/kagg-compatibility"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.11.1",
    "phpcompatibility/php-compatibility": "^9.3.5",
    "phpcompatibility/phpcompatibility-wp": "^2.1.5",
    "wp-coding-standards/wpcs": "^3.1.0"
  },
  "autoload": {
    "psr-4": {
      "KAGG\\Compatibility\\": "src/php",
      "KAGG\\Compatibility\\Settings\\Abstracts\\": "src/php/Settings/Abstracts"
    }
  },
  "scripts": {
    "phpcs": "vendor/bin/phpcs --colors --standard=phpcs.xml"
  }
}
