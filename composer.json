{
  "name": "kagg/compatibility",
  "description": "KAGG Compatibility",
  "license": "GPL-2.0",
  "keywords": [
    "compatibility",
    "PHP 8.1",
    "development",
    "deprecation",
    "notice"
  ],
  "homepage": "https://wordpress.org/plugins/kagg-compatibility/",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "KAGG Design",
      "email": "info@kagg.eu",
      "homepage": "https://kagg.eu/en/"
    }
  ],
  "support": {
    "issues": "https://wordpress.org/support/plugin/kagg-compatibility"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "5.6"
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.7.2",
    "phpcompatibility/php-compatibility": "^9.3.5",
    "phpcompatibility/phpcompatibility-wp": "^2.1.4",
    "wp-coding-standards/wpcs": "^2.3.0"
  },
  "autoload": {
    "psr-4": {
      "KAGG\\Compatibility\\": "src/php"
    }
  },
  "scripts": {
    "phpcs": "vendor/bin/phpcs --colors"
  }
}
